{
  "expo": {
    "name": "GitHub Repo Search",
    "slug": "github-repo-search",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This basic app allows you to type a search and display informations about GitHub repositories",
    "githubUrl": "https://github.com/adrienzagar/React-Native-GitHub-Repositry-Search"
  }
}
